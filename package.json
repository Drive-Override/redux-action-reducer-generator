{
  "name": "actiongenerator",
  "version": "1.0.0",
  "description": "A generator function to automate the creation of redux actions and reducers",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "build": "webpack",
    "release:patch": "better-npm-run release:patch",
    "release:minor": "better-npm-run release:minor",
    "release:major": "better-npm-run release:major"
  },
  "betterScripts": {
    "release:patch": {
      "command": "release-it env.patch",
      "env": {
        "GITHUB_TOKEN": ""
      }
    },
    "release:minor": {
      "command": "release-it env.minor",
      "env": {
        "GITHUB_TOKEN": ""
      }
    },
    "release:major": {
      "command": "release-it env.major",
      "env": {
        "GITHUB_TOKEN": ""
      }
    }
  },
  "keywords": [
    "redux",
    "action",
    "generator",
    "reducer",
    "generator"
  ],
  "author": "Philipp Rost",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "babel-loader": "^8.0.5",
    "better-npm-run": "^0.1.1",
    "expect.js": "^0.3.1",
    "mocha": "^6.0.2",
    "release-it": "^10.3.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  }
}
